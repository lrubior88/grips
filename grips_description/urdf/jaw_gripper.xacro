<?xml version="1.0"?>
<robot xmlns:xacro="http://www.ros.org/wiki/xacro">
  <!-- Links Mass -->
  <property name="gripper_base_mass" value="0.923" />
  <property name="outer_mass" value="0.091" />
  <property name="inner_mass" value="0.194" />
  <property name="finger_mass" value="0.174" />
  <!-- Links COG -->
  <property name="gripper_base_cog" value="0 26.28e-3 0" />
  <property name="outer_cog" value="0 31.725e-3 0" />
  <property name="r_inner_cog" value="-0.316e-3 23.873e-3 0" />
  <property name="l_inner_cog" value="0.316e-3 23.873e-3 0" />
  <property name="r_finger_cog" value="3.441e-3 20.353e-3 0" />
  <property name="l_finger_cog" value="-3.441e-3 20.353e-3 0" />  
  
  <!-- Meshes -->
  <property name="gripper_base_mesh" value="package://grips_description/meshes/visual/gripper_base.dae" />
  <property name="outer_mesh" value="package://grips_description/meshes/visual/outer_pivot.dae" />
  <property name="r_inner_mesh" value="package://grips_description/meshes/visual/r_inner_pivot.dae" />
  <property name="l_inner_mesh" value="package://grips_description/meshes/visual/l_inner_pivot.dae" />
  <property name="r_finger_mesh" value="package://grips_description/meshes/visual/r_finger.dae" />
  <property name="l_finger_mesh" value="package://grips_description/meshes/visual/l_finger.dae" />
  <!-- Joints Dynamics -->
  <property name="outer_damping" value="0.1" />
  <property name="inner_damping" value="0.1" />
  <property name="finger_damping" value="0.1" />
  <property name="inner_friction" value="0" />
  <property name="outer_friction" value="0" />
  <property name="finger_friction" value="0" />
  
  <!-- Links Inertia -->
  <macro name="gripper_base_inertia">
    <inertia ixx="9.67e-4" ixy="0.0" ixz="0.0" iyy="0.002" iyz="0.0"
      izz="0.002" />
  </macro>
  <macro name="outer_inertia">
    <inertia ixx="4.664e-5" ixy="0.0" ixz="0.0" iyy="5.519e-6"
      iyz="0.0" izz="4.682e-5" />
  </macro>
  <macro name="r_inner_inertia">
    <inertia ixx="1.179e-4" ixy="1.347e-6" ixz="0.0" iyy="5.647e-5"
      iyz="0.0" izz="7.353e-5" />
  </macro>
  <macro name="l_inner_inertia">
    <inertia ixx="1.179e-4" ixy="-1.347e-6" ixz="0.0" iyy="5.647e-5"
      iyz="0.0" izz="7.353e-5" />
  </macro>
  <macro name="r_finger_inertia">
    <inertia ixx="1.65e-4" ixy="3.16e-5" ixz="0.0" iyy="3.595e-5"
      iyz="0.0" izz="1.701e-4" />
  </macro>
  <macro name="l_finger_inertia">
    <inertia ixx="1.65e-4" ixy="-3.16e-5" ixz="0.0" iyy="3.595e-5"
      iyz="0.0" izz="1.701e-4" />
  </macro>
  
  <macro name="simple_gripper" params="name parent">
    <!-- base link -->
    <create_link name="${name}gripper_base" color="metal_seamed" mesh="${gripper_base_mesh}">
      <collision>
        <geometry>
          <mesh filename="${gripper_base_mesh}" />
        </geometry>
      </collision>
      <inertial>
        <origin xyz="${gripper_base_cog}" rpy="0 0 0" />
        <mass value="${gripper_base_mass}" />
        <gripper_base_inertia />
      </inertial>
    </create_link>
    <!-- outer links -->
    <create_link name="${name}r_outer" color="metal_seamed" mesh="${outer_mesh}">
      <collision>
        <geometry>
          <mesh filename="${gripper_base_mesh}" />
        </geometry>
      </collision>
      <inertial>
        <origin xyz="${outer_cog}" rpy="0 0 0" />
        <mass value="${outer_mass}" />
        <outer_inertia />
      </inertial>
    </create_link>
    <create_link name="${name}l_outer" color="metal_seamed" mesh="${outer_mesh}">
      <collision>
        <geometry>
          <mesh filename="${outer_mesh}" />
        </geometry>
      </collision>
      <inertial>
        <origin xyz="${outer_cog}" rpy="0 0 0" />
        <mass value="${outer_mass}" />
        <outer_inertia />
      </inertial>
    </create_link>
    <!-- inner links -->
    <create_link name="${name}r_inner" color="metal_seamed" mesh="${r_inner_mesh}">
      <collision>
        <geometry>
          <mesh filename="${r_inner_mesh}" />
        </geometry>
      </collision>
      <inertial>
        <origin xyz="${r_inner_cog}" rpy="0 0 0" />
        <mass value="${inner_mass}" />
        <r_inner_inertia />
      </inertial>
    </create_link>
    <create_link name="${name}l_inner" color="metal_seamed" mesh="${l_inner_mesh}">
      <collision>
        <geometry>
          <mesh filename="${l_inner_mesh}" />
        </geometry>
      </collision>
      <inertial>
        <origin xyz="${l_inner_cog}" rpy="0 0 0" />
        <mass value="${inner_mass}" />
        <l_inner_inertia />
      </inertial>
    </create_link>
    <!-- finger links -->
    <create_link name="${name}r_finger" color="metal_seamed" mesh="${r_finger_mesh}">
      <collision>
        <geometry>
          <mesh filename="${r_finger_mesh}" />
        </geometry>
      </collision>
      <inertial>
        <origin xyz="${r_finger_cog}" rpy="0 0 0" />
        <mass value="${finger_mass}" />
        <r_finger_inertia />
      </inertial>
    </create_link>
    <create_link name="${name}l_finger" color="metal_seamed" mesh="${l_finger_mesh}">
      <collision>
        <geometry>
          <mesh filename="${l_finger_mesh}" />
        </geometry>
      </collision>
      <inertial>
        <origin xyz="${l_finger_cog}" rpy="0 0 0" />
        <mass value="${finger_mass}" />
        <l_finger_inertia />
      </inertial>
    </create_link>

    <!-- joints -->
    <joint name="${name}gripper_base_joint" type="fixed">
      <origin xyz="0 0 0" rpy="0 0 0" />
      <parent link="${parent}" />
      <child link="${name}gripper_base" />
    </joint>
    
    <!-- Fixed Joints -->
    <joint name="${name}r_inner" type="fixed">
      <child link="${name}r_inner" />
      <parent link="${name}gripper_base" />
      <origin xyz="31.737e-3 42.19e-3 0" rpy="0 0 0" />
    </joint>
    <joint name="${name}l_inner" type="fixed">
      <child link="${name}l_inner" />
      <parent link="${name}gripper_base" />
      <origin xyz="-31.737e-3 42.19e-3 0" rpy="0 0 0" />
    </joint>
    <joint name="${name}r_outer" type="fixed">
      <child link="${name}r_outer" />
      <parent link="${name}gripper_base" />
      <origin xyz="58.93e-3 26.49e-3 0" rpy="0 0 0" />
    </joint>
    <joint name="${name}l_outer" type="fixed">
      <child link="${name}l_outer" />
      <parent link="${name}gripper_base" />
      <origin xyz="-58.93e-3 26.49e-3 0" rpy="0 0 0" />
    </joint>
<!--
    <joint name="${name}r_finger" type="fixed">
      <child link="${name}r_finger" />
      <parent link="${name}r_inner" />
      <origin xyz="0 63.45e-3 0" rpy="0 0 0" />
    </joint>
    <joint name="${name}l_finger" type="fixed">
      <child link="${name}l_finger" />
      <parent link="${name}l_inner" />
      <origin xyz="0 63.45e-3 0" rpy="0 0 0" />
    </joint>
-->

    <!-- Uncommented to use non fixed joints -->
<!--
    <joint name="${name}r_inner" type="revolute">
      <child link="${name}r_inner" />
      <parent link="${name}gripper_base" />
      <axis xyz="0 0 1" />
      <origin xyz="31.737e-3 42.19e-3 0" rpy="0 0 0" />
      <limit lower="-1.43" upper="0.4" effort="${gripper_effort}" velocity="${gripper_velocity}" />
      <dynamics damping="${inner_damping}" />
    </joint>
    <joint name="${name}l_inner" type="revolute">
      <child link="${name}l_inner" />
      <parent link="${name}gripper_base" />
      <axis xyz="0 0 1" />
      <origin xyz="-31.737e-3 42.19e-3 0" rpy="0 0 0" />
      <limit lower="-0.4" upper="1.43" effort="${gripper_effort}" velocity="${gripper_velocity}" />
      <dynamics damping="${inner_damping}" />
    </joint>
-->
<!--
    <joint name="${name}r_outer" type="revolute">
      <child link="${name}r_outer" />
      <parent link="${name}gripper_base" />
      <axis xyz="0 0 1" />
      <origin xyz="58.93e-3 26.49e-3 0" rpy="0 0 0" />
      <limit lower="-1.43" upper="0.4" effort="${gripper_effort}" velocity="${gripper_velocity}" />
      <dynamics damping="${outer_damping}" />
    </joint>
    <joint name="${name}l_outer" type="revolute">
      <child link="${name}l_outer" />
      <parent link="${name}gripper_base" />
      <axis xyz="0 0 1" />
      <origin xyz="-58.93e-3 26.49e-3 0" rpy="0 0 0" />
      <limit lower="-0.4" upper="1.43" effort="${gripper_effort}" velocity="${gripper_velocity}" />
      <dynamics damping="${outer_damping}" />
    </joint>
-->
    <joint name="${name}r_finger" type="revolute">
      <child link="${name}r_finger" />
      <parent link="${name}r_inner" />
      <axis xyz="0 0 1" />
      <origin xyz="0 63.45e-3 0" rpy="0 0 0" />
      <limit lower="${-pi/2}" upper="${pi/2}" effort="${gripper_effort}" velocity="${gripper_velocity}" />
      <dynamics damping="${finger_damping}" />
    </joint>
    <joint name="${name}l_finger" type="revolute">
      <child link="${name}l_finger" />
      <parent link="${name}l_inner" />
      <axis xyz="0 0 1" />
      <origin xyz="0 63.45e-3 0" rpy="0 0 0" />
      <limit lower="${-pi/2}" upper="${pi/2}" effort="${gripper_effort}" velocity="${gripper_velocity}" />
      <dynamics damping="${finger_damping}" />
    </joint>

    <!-- transmissions -->
<!--
    <simple_transmission joint="${name}r_outer" />
    <simple_transmission joint="${name}l_outer" />
    <simple_transmission joint="${name}r_inner" />
    <simple_transmission joint="${name}l_inner" />
-->
    <simple_transmission joint="${name}r_finger" />
    <simple_transmission joint="${name}l_finger" />
    
    <!-- links -->
    <gazebo reference="${name}gripper_base">
      <material>Telerobcar/MetalSeamed</material>
      <selfCollide>false</selfCollide>
    </gazebo>
    <gazebo reference="${name}r_outer">
      <material>Telerobcar/MetalSeamed</material>
      <selfCollide>false</selfCollide>
    </gazebo>
    <gazebo reference="${name}l_outer">
      <material>Telerobcar/MetalSeamed</material>
      <selfCollide>false</selfCollide>
    </gazebo>
    <gazebo reference="${name}r_inner">
      <material>Telerobcar/MetalSeamed</material>
      <selfCollide>false</selfCollide>
    </gazebo>
    <gazebo reference="${name}l_inner">
      <material>Telerobcar/MetalSeamed</material>
      <selfCollide>false</selfCollide>
    </gazebo>
    <gazebo reference="${name}r_finger">
      <material>Telerobcar/MetalSeamed</material>
      <selfCollide>false</selfCollide>
      <mu1>1.1</mu1>
      <mu2>1.1</mu2>
      <fdir1>0 0 1</fdir1>
      <kp>1000000.0</kp>
      <kd>10.0</kd>
      <maxVel>0.1</maxVel>
      <minDepth>0.001</minDepth>
      <maxContacts>5</maxContacts>
    </gazebo>
    <gazebo reference="${name}l_finger">
      <material>Telerobcar/MetalSeamed</material>
      <selfCollide>false</selfCollide>
      <mu1>1.1</mu1>
      <mu2>1.1</mu2>
      <fdir1>0 0 1</fdir1>
      <kp>1000000.0</kp>
      <kd>100.0</kd>
      <maxVel>0.1</maxVel>
      <minDepth>0.001</minDepth>
      <maxContacts>5</maxContacts>
    </gazebo>	
    <gazebo>
      <gripper name="${name}grasping">
        <grasp_check>
          <attach_steps>1</attach_steps>    <!-- default: 20 -->
          <detach_steps>1</detach_steps>    <!-- default: 40 -->
          <min_contact_count>2</min_contact_count>
        </grasp_check>
        <gripper_link>${name}r_finger</gripper_link>
        <gripper_link>${name}l_finger</gripper_link>
        <palm_link>${name}gripper_base</palm_link>
      </gripper>
    </gazebo>
  </macro>
</robot>
